<%- include ("partials/header.ejs"); %>

<h1>Home</h1>
<p><%= homeStartingContent %></p>

<ul class="Posts">
  <% for (var i = 0; i < posts.length; i++) { %>
  <li class="timeline-yellow">
    <div class="timeline-time">
      <span
        class="time"
        style="
          font-weight: 700;
          font-size: 25px;
          line-height: 20px;
          text-align: left;
        "
      >
        <%= posts[i].Title %>
      </span>
      <span class="date" style="text-align: left"> <%= posts[i].Body %> </span>
    </div>
  </li>
  <% } %>
</ul>

<%- include ("partials/footer.ejs"); %>
